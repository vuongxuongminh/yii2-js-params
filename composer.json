{
    "name": "vxm/yii2-js-params",
    "description": "Support passed variables to javascript.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","passed-variables"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev": {
        "yiisoft/yii2-smarty": "~2.0.0",
        "yiisoft/yii2-twig": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "vxm\\jsParams\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bootstrap": "vxm\\jsParams\\ViewRenderer"
    }
}
